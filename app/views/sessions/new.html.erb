<h1 align="center">Log in</h1>


<%= form_for(:session, :html => {class: "form-horizontal", role: "form"}, url: login_path) do |f| %>

        
    
    <div class="form-group">
        <%= f.label "Email" %><br/>
        <%= f.email_field :email, class:"form-control", autofocus: true, placeholder:"Enter email" %>
    </div>
    
    <div class="form-group">
        <%= f.label :password %><br/>
        <%= f.password_field :password, class:"form-control", autofocus: true, autocomplete: "off", placeholder:"Enter password" %>
    </div>
    
    <div class="form-group">
        <%= f.submit "Log in", class:"btn btn-primary" %>
    </div>
    
    
<% end %>
    
  