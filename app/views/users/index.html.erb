<h1 align="center">All Users</h1>

<div align="center">
    <%= will_paginate %>
</div>
<table class="table table-hover">
    <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Line Manager ID</th>
        
         
    </tr>
    
    <% @users.each do |u| %>
        <tr>
            <td>
                <%= link_to u.username, user_path(u) %>
            </td>
            <td>
                <%= u.email %>
            </td>
            <td>
                <%= u.linemanager %>
            </td>
          
            
        </tr>
    <% end %>
</table>
<div align="center">
    <%= will_paginate %>
</div>